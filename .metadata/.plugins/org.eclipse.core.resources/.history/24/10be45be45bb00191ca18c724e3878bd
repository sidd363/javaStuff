package coreJava;

public class LambdaEx {
	
	class Developers{
		String name;
		public Developers(String name, int salary, int age) {
			this.name = name;
			this.salary = salary;
			this.age = age;
		}
		int salary,age;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		LambdaEx lx = new LambdaEx();
		Developers d1 = lx.new Developers("siddhant", 90000, 27);
		Developers d2 = lx.new Developers("arora", 60000, 26);
		Developers d3 = lx.new Developers("nimish", 100000, 25);
		
		System.out.println("hello wprld");
		
